<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.myappcompany.jp.findcoronavaccine">

    <application
        android:allowBackup="true"
        android:icon="@drawable/corona_vaccine_finder_logo1"
        android:label="@string/app_name"
        android:roundIcon="@drawable/corona_vaccine_finder_logo1"
        android:supportsRtl="true"
        android:theme="@style/Theme.FindCoronaVaccine">
        <activity android:name=".Calendar_Activity"></activity>
        <activity android:name=".SearchByPincode_activity" />
        <activity android:name=".SearchByDistrictName_activity" />
        <activity android:name=".VaccinationList_activity"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

</manifest>